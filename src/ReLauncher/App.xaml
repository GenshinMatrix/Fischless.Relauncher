<Application x:Class="Relauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Relauncher"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
                <ResourceDictionary Source="/Views/Controls/Button/Button.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="DelugeLEDFontFamily">pack://application:,,,/Relauncher;component/Assets/Fonts/deluge-led.ttf#Deluge LED</FontFamily>
            <FontFamily x:Key="SymbolThemeFontFamily">pack://application:,,,/Relauncher;component/Assets/Fonts/Segoe Fluent Icons.ttf#Segoe Fluent Icons</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
